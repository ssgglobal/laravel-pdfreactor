{
    "name": "ssgglobal/php-component",
    "description": "A template for creating PHP components",
    "type": "library",
    "require": {
        "php": ">= 7.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "prefer-stable": true,
    "license": "proprietary",
    "authors": [
        {
            "name": "DS Engineering Team",
            "email": "engineers@stepstoneglobal.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "StepStone\\Component\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StepStone\\Component\\Tests\\": "tests/"
        }
    },
    "repositories": [{
        "type": "composer",
        "url": "https://satis.dseapps.com/"
    }]
}